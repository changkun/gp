// DEFINITIONS
int _pointsPerSide;
float _time;
float _scale;
float _surfaceDensity;

struct Vert {
    float3 pos;
    float3 normal;
    int index;
};

struct CubeData {
    int infos;
    Vert verts[12];
    int3 faces[5];
};

RWStructuredBuffer<CubeData> _cubeDataOutput;

//FUNCTIONS
float calculateDensity(float x, float y, float z, float time) {
    float3 center1 = float3(0.5 + sin(time) * 0.25, 0.5 + cos(time) * 0.25, 0.5);
    float sphere1 = -length(center1 - float3(x,y,z)) + 1.1;
    return sphere1;
}

float3 getCoord(int3 pos, int pointsPerSide) {
    return float3(float(pos.x) / float(pointsPerSide - 1), float(pos.y) / float(pointsPerSide - 1), float(pos.z) / float(pointsPerSide - 1));
}

float getDensity(float3 coord, float time) {
    return calculateDensity(coord.x, coord.y, coord.z, time);
}

bool isMeshEdge(bool corner1Inside, bool corner2Inside) {
    return ((corner1Inside && !corner2Inside) || (!corner1Inside && corner2Inside));
}

float3 getScaledPosition(float3 coord, float scale) {
    return coord * scale;
}

float3 getInterpolatedPosition(int corners[2], float scale, float surfaceDensity, float3 cornerCoords[8], float cornerDensities[8]) {
    float3 p1 = getScaledPosition(cornerCoords[corners[0]], scale);
    float3 p2 = getScaledPosition(cornerCoords[corners[1]], scale);
    float t = (surfaceDensity - cornerDensities[corners[0]]) / (cornerDensities[corners[1]] - cornerDensities[corners[0]]);
    return p1 + t * (p2 -p1);
}

int calculateVertIndex(int pointDirGroup[2], int pointsPerSide, int3 cubeCorners[8]) {
    return (int(cubeCorners[pointDirGroup[0]].x) + int(cubeCorners[pointDirGroup[0]].z) * pointsPerSide + int(cubeCorners[pointDirGroup[0]].y) * pointsPerSide * pointsPerSide) * 4 + pointDirGroup[1];
}

[numthreads(4,4,4)]
void CSMain (uint3 id : SV_DispatchThreadID)
{
    int cubesPerSide = _pointsPerSide - 1;
    if (int(id.x) >= cubesPerSide || int(id.y) >= cubesPerSide || int(id.z) >= cubesPerSide) {
        return;
    }
    int cubeID = int(id.x) + int(id.z) * cubesPerSide + int(id.y) * cubesPerSide * cubesPerSide;

    int3 cubeCorners[8] = {
        uint3(id.x, id.y, id.z),
        uint3(id.x + 1, id.y, id.z),
        uint3(id.x + 1, id.y, id.z + 1),
        uint3(id.x, id.y, id.z + 1),
        uint3(id.x, id.y + 1, id.z),
        uint3(id.x + 1, id.y + 1, id.z),
        uint3(id.x + 1, id.y + 1, id.z + 1),
        uint3(id.x, id.y + 1, id.z + 1),
    };

    float3 cornerCoords[8] = {
        getCoord(cubeCorners[0], _pointsPerSide),
        getCoord(cubeCorners[1], _pointsPerSide),
        getCoord(cubeCorners[2], _pointsPerSide),
        getCoord(cubeCorners[3], _pointsPerSide),
        getCoord(cubeCorners[4], _pointsPerSide),
        getCoord(cubeCorners[5], _pointsPerSide),
        getCoord(cubeCorners[6], _pointsPerSide),
        getCoord(cubeCorners[7], _pointsPerSide)
    };

    float cornerDensities[8] = {
        getDensity(cornerCoords[0], _time),
        getDensity(cornerCoords[1], _time),
        getDensity(cornerCoords[2], _time),
        getDensity(cornerCoords[3], _time),
        getDensity(cornerCoords[4], _time),
        getDensity(cornerCoords[5], _time),
        getDensity(cornerCoords[6], _time),
        getDensity(cornerCoords[7], _time),
    };

    bool isCornerInside[8] = {
        cornerDensities[0] >= _surfaceDensity,
        cornerDensities[1] >= _surfaceDensity,
        cornerDensities[2] >= _surfaceDensity,
        cornerDensities[3] >= _surfaceDensity,
        cornerDensities[4] >= _surfaceDensity,
        cornerDensities[5] >= _surfaceDensity,
        cornerDensities[6] >= _surfaceDensity,
        cornerDensities[7] >= _surfaceDensity,
    };

    int cubeIndex = 0;
    if (isCornerInside[0]) cubeIndex |= 1;
    if (isCornerInside[1]) cubeIndex |= 2;
    if (isCornerInside[2]) cubeIndex |= 4;
    if (isCornerInside[3]) cubeIndex |= 8;
    if (isCornerInside[4]) cubeIndex |= 16;
    if (isCornerInside[5]) cubeIndex |= 32;
    if (isCornerInside[6]) cubeIndex |= 64;
    if (isCornerInside[7]) cubeIndex |= 128;

    bool vertSet[12] = {
        false, false, false, false, false, false, false, false, false, false, false, false
    };
    int vertsUsed = 0;
    // create Faces
    int faceCount = 0;
    for (int i = 0; triangulation[cubeIndex][i] != -1; i+=3) {
        int triangleVerts[3] = {
            int(triangulation[cubeIndex][i]),
            int(triangulation[cubeIndex][i+1]),
            int(triangulation[cubeIndex][i+2])
        };
        [unroll(3)]
        for (int j = 0; j < 3; j++) { // add new verts
            if (!vertSet[triangleVerts[j]]) {
                _cubeDataOutput[cubeID].verts[triangleVerts[j]].pos = getInterpolatedPosition(cubeEdgesForVerts[triangleVerts[j]], _scale, _surfaceDensity, cornerCoords, cornerDensities);
                _cubeDataOutput[cubeID].verts[triangleVerts[j]].index = calculateVertIndex(vertBasePointAndDir[triangleVerts[j]], _pointsPerSide, cubeCorners);
                _cubeDataOutput[cubeID].verts[triangleVerts[j]].normal = float3(0,0,0);
                vertSet[triangleVerts[j]] = true;
                vertsUsed = vertsUsed + pow(2, triangleVerts[j]);
            }
        }
        // calculate face normal
        float3 a = _cubeDataOutput[cubeID].verts[triangleVerts[2]].pos - _cubeDataOutput[cubeID].verts[triangleVerts[1]].pos;
        float3 b = _cubeDataOutput[cubeID].verts[triangleVerts[0]].pos - _cubeDataOutput[cubeID].verts[triangleVerts[1]].pos;
        float3 n = normalize(cross(a,b));
        // add normal to each vert
        [unroll(3)]
        for (int k = 0; k < 3; k++) {
            _cubeDataOutput[cubeID].verts[triangleVerts[k]].normal = _cubeDataOutput[cubeID].verts[triangleVerts[k]].normal + n;
        }
        _cubeDataOutput[cubeID].faces[faceCount] = int3(triangleVerts[0], triangleVerts[1], triangleVerts[2]);
        faceCount = faceCount + 1;
    }
    vertsUsed = vertsUsed * 16; // move bits up by 4#
    // normalize normal vectors
    [unroll(12)]
    for (int l = 0; l < 12; l++) {
        if (vertSet[l]) {
            _cubeDataOutput[cubeID].verts[l].normal = normalize(_cubeDataOutput[cubeID].verts[l].normal);
        }
    }

    int cubeDataIndex = vertsUsed;

    faceCount = faceCount * 2; // move bits up by one
    cubeDataIndex = cubeDataIndex + faceCount;
    if (cubeIndex > 0 && cubeIndex < 255) {
        cubeDataIndex = cubeDataIndex + 1; // set first bit
    }
    
    int movedUpCubeIndex = cubeIndex * 65536; // move up the cube index
    cubeDataIndex = cubeDataIndex + movedUpCubeIndex;
    _cubeDataOutput[cubeID].infos = cubeDataIndex;
}
